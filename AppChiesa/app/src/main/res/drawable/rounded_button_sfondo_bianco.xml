<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="50dp" />
            <solid
                android:color="@color/LightManagerGreen"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="50dp" />
            <stroke
                android:width="2dp"
                android:color="@color/LightManagerBlue"/>
            <solid
                android:color="@color/white"/>
        </shape>
    </item>
</selector>